
#include <iostream>
using namespace std;
int main() {
    int n;
    cout<<"Enter number of elements : ";
    cin>>n;
    int a[n];
    cout<<"Enter your elements \n";
    for(int i =0;i<n;i++) {
        cin>>a[i];
    }
    int pos;
    cout<<"Enter the position of the element you want to delete : ";
    cin>>pos;
    if(pos==n) {
        for(int i =0;i<n-1;i++) {
            cout<<a[i]<<" ";
        }
    } else {
        for(int i=pos-1;i<n-1;i++) {
            a[i]=a[i+1];
        }
        for(int i =0;i<n-1;i++) {
            cout<<a[i]<<" ";
        }
    }
    return 0;
}
