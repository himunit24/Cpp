
#include <iostream>
using namespace std;
int main() {
    int n;
    cout<<"Enter number of elements : ";
    cin>>n;
    int a[n];
    cout<<"Enter your elements \n";
    for(int i =0;i<n;i++) {
        cin>>a[i];
    }
    int pos,key;
    cout<<"Enter the position of the element to insert : ";
    cin>>pos;
    cout<<"Enter element you want to insert : ";
    cin>>key;
    
    for(int i=n;i>pos-1;i--) {
        a[i]=a[i-1];
    }
    a[pos-1]=key;
    for(int i =0;i<n+1;i++) {
        cout<<a[i]<<" ";
    }
    return 0;
}
