
#include <iostream>
using namespace std;
int main() {
    int n;
    cout<<"Enter number of elements : ";
    cin>>n;
    int a[n];
    cout<<"Enter your elements \n";
    for(int i =0;i<n;i++) {
        cin>>a[i];
    }
    int l1=a[0];
    int l2=a[0];
    for(int i=1;i<n;i++) {
        if(a[i]>l1) {
            l2=l1;
            l1=a[i];
        } else if(a[i]>l2 && a[i]<l1) {
            l2=a[i];
        }
    }
    if(l1==l2) {
        cout<<"Largest is "<<l1<<" No second largest found!";
    } else {
       cout<<"Largest elements is "<<l1<<" and second largest is "<<l2; 
    }
    

    return 0;
}
